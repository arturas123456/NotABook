<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Width="927" Height="618"
        x:Class="NotABook.MainWindow"
		xmlns:local="clr-namespace:NotABook"
		xmlns:control="using:NotABook"
        Title="NotABook" Background="{ DynamicResource Background900 }">
	<Grid Width="900" Height="600">	
		<!-- Grid shown on startup -->
		<local:StartPage />

		<!-- Grid shown on note selection -->
		<local:NotePage />
	</Grid>
</Window>


