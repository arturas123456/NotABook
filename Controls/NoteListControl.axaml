<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:local="clr-namespace:NotABook"
			 xmlns:controls="using:NotABook"
             mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="300"
             x:Class="NotABook.NoteListControl">
	<ListBox Background="{ DynamicResource Background800 }" Name="NotePanel">
		<Label Content="Note List"></Label>
		<local:NoteListItemControl />
		<controls:NoteListItemControl NoteTitle="Note 1" NoteDate="2021-01-01" NoteWidth="600"/>
		<controls:NoteListItemControl NoteTitle="Note 2" NoteDate="2021-01-02" NoteWidth="600"/>
		<controls:NoteListItemControl NoteTitle="Note 3" NoteDate="2021-01-03" NoteWidth="600"/>
	</ListBox>
</UserControl>
